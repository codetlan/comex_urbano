<div class="hero-intro">
  <div class="wrapper">
    <div class="cols-12">
      <div class="col col-12">
        <div class="col-inner">
          <div class="slider">

            <div class="featured-post">
              <div class="image">
                <%= link_to '/videos/1' do %>
                    <i class="i play"></i>
                    <%= image_tag 'placeholder/featured-image.jpg' %>
                <% end %>
              </div>
              <div class="description">
                <div class="category">
                  <%= link_to '/videos/1' do %>
                      <p>Festival Internacional Comex urbano</p>
                  <% end %>
                </div>
                <div class="title">
                  <%= link_to '/videos/1' do %>
                    <p>FICU es un festival que da apertura al arte urbano y digital en México</p>
                  <% end %>
                </div>
              </div>
            </div>
            <div class="featured-post">
              <div class="image">
                <%= link_to '/videos/2' do %>
                    <i class="i play"></i>
                    <%= image_tag 'placeholder/featured-image2.jpg' %>
                <% end %>
              </div>
              <div class="description">
                <div class="category">
                  <%= link_to '/videos/2' do %>
                      <p>Festival Internacional Arte urbano</p>
                  <% end %>
                </div>
                <div class="title">
                  <%= link_to '/videos/2' do %>
                      <p>FIC</p>
                  <% end %>
                </div>
              </div>
            </div>

          </div>
        </div>
      </div>
    </div>
  </div>
</div>
<div class="filters-wrap">
  <div class="wrapper">
    <div class="cols-12">
      <div class="col col-12">
        <div class="col-inner">
          <div class="filters">
