<h1>Home Admin</h1>

<table>
  <thead>
    <tr>
      <th>Facebook</th>
      <th>Twitter</th>
      <th>Youtube</th>
      <th>Vimeo</th>
      <th>Copyright</th>
      <th>Terms</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @landing_pages.each do |landing_page| %>
      <tr>
        <td><%= landing_page.facebook %></td>
        <td><%= landing_page.twitter %></td>
        <td><%= landing_page.youtube %></td>
        <td><%= landing_page.vimeo %></td>
        <td><%= landing_page.copyright %></td>
        <td><%= landing_page.terms %></td>
        <td><%= link_to 'Show', landing_page %></td>
        <td><%= link_to 'Edit', edit_landing_page_path(landing_page) %></td>
        <td><%= link_to 'Destroy', landing_page, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

