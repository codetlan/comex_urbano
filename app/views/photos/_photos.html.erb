<table>
  <thead>
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th>Image</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>
  </thead>

  <tbody>
  <% @photos.each do |photo| %>
      <tr>
        <td><%= photo.name %></td>
        <%=  photo.description %>
        <td><a href="/photos/<%= photo.id.to_s %>"><%= image_tag photo.image.url(:square) %></a></td>
      </tr>
  <% end %>
  </tbody>
</table>

<br>