<% @section.each do |section| %>
    <div class="section-heading">
      <div class="wrapper">
        <div class="cols-12">
          <div class="col col-12">
            <div class="col-inner">
              <%= image_tag section.main_banner.url(:original) %>
            </div>
          </div>
        </div>
      </div>
    </div>
    <div class="filters-wrap">
      <div class="wrapper">
        <div class="cols-12">
          <div class="col col-12">
            <div class="col-inner">
              <div class="filters">
                <div class="page-navigation">
                  <ul>
                    <li>
                      <%= link_to 'Ultimos videos', '#', class: 'active' %>
                    </li>
                    <li>
