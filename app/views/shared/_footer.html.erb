          <div class="col-inner">
            <ul>
              <li class="footer-nav-item">
                <%= link_to "Video", "#" %>
              </li>
              <li class="footer-nav-item">
                <%= link_to "Galería", "#" %>
              </li>
              <li class="footer-nav-item">
                <%= link_to "Blog", "#" %>
