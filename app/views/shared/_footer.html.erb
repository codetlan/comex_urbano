  <div class="footer-brand">
    <div class="wrapper">
      <div class="cols-12">
        <div class="col col-12">
          <div class="col-inner">
            <%= link_to 'http://www.comex.com.mx/', target: "_blank" do %>
              <%= image_tag 'comex-logo.png' %>
