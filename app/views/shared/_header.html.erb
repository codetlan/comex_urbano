<div class="header">
  <div class="wrapper">
    <div class="cols-12">
      <div class="col col-12">
        <div class="col-inner">
          <div class="brand">
            <%= link_to root_path do %>
              <%= image_tag 'comex-urbano-logo.png' %>
            <% end %>
          </div>
          <div class="header-right-content">
            <div class="mobile-menu">
              <%= image_tag 'menu.png' %>
            </div>
            <div class="comex-logo-brand">
              <%= link_to 'http://www.comex.com.mx/', target: "_blank" do %>
                <%= image_tag 'comex-logo.png' %>
              <% end %>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>

<div class="site-links">
  <div class="wrapper">
    <div class="cols-12">
      <div class="col col-12">
        <div class="col-inner">
          <div class="site-navigation">
            <div class="navigation">
              <ul>
                <li><a href="">Videos</a></li>
                <li><a href="">Galería</a></li>
                <li><a href="">Blog</a></li>
                <li><a href="">¿Qué es Comex urbano?</a></li>
              </ul>
            </div>
            <div class="social-links">
              <ul>
                <li><%= link_to image_tag('fb.png'), "#" %></li>
                <li><%= link_to image_tag('twitter.png'), "#" %></li>
                <li><%= link_to image_tag('youtube.png'), "#" %></li>
                <li><%= link_to image_tag('vimeo.png'), "#" %></li>
              </ul>
            </div>
          </div>
          
          <div class="site-search">
            <form action="#" class="search-form">
              <input type="text" placeholder="Buscar">
            </form>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
