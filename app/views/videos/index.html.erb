<%= render partial: 'shared/section_youtube_subscribe' %>
<div class="videos-list">
  <div class="wrapper">
    <div class="cols-12">
      <% @videos.each do |video| %>
          <div class="col col-4">
            <div class="col-inner">
              <div class="video-box">
                <div class="video">
                  <%= link_to '/videos/'+video.id.to_s do %>
                      <!--<i class="i play"></i>-->
                      <iframe width="100%" height="100%" src="<%= video.youtube_embed(video.link) %>?&showinfo=0&autohide=1"></iframe>
                <% end %>
                </div>
                <div class="video-description">
                  <%= link_to '/videos/'+video.id.to_s do %>
                      <h3><%= video.name %></h3>
                  <% end %>
                  <p><%= truncate(video.description, :length => 50) %></p>
                </div>
              </div>
            </div>
          </div>
      <% end %>
    </div>
  </div>
</div>

